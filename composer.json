{
	"name": "typo3/cms-base-distribution",
	"description": "TYPO3 CMS Base Distribution",
	"license": "GPL-2.0-or-later",
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true,
			"helhum/dotenv-connector": true
		}
	},
	"repositories": [
		{
			"type": "path",
			"url": "packages/*"
		},
		{
			"type": "composer",
			"url": "https://composer.typo3.org/"
		}
	],
	"require": {
        "b13/assetcollector": "*",
		"b13/container": "*",
		"b13/picture": "*",
		"helhum/dotenv-connector": "*",
		"helhum/typo3-console": "*",
		"ichhabrecht/content-defender": "*",
		"mst/mst-site": "dev-main",
		"mst/mst-reloadtrigger": "*",
        "typo3/cms-backend": "*",
        "typo3/cms-belog": "*",
        "typo3/cms-beuser": "*",
        "typo3/cms-core": "^12.4",
        "typo3/cms-dashboard": "*",
        "typo3/cms-extbase": "*",
        "typo3/cms-extensionmanager": "*",
        "typo3/cms-felogin": "*",
        "typo3/cms-filelist": "*",
        "typo3/cms-fluid": "*",
        "typo3/cms-fluid-styled-content": "*",
        "typo3/cms-form": "*",
        "typo3/cms-frontend": "*",
        "typo3/cms-impexp": "*",
        "typo3/cms-info": "*",
        "typo3/cms-install": "*",
        "typo3/cms-lowlevel": "^12.4",
        "typo3/cms-reactions": "*",
        "typo3/cms-rte-ckeditor": "*",
        "typo3/cms-seo": "*",
        "typo3/cms-setup": "*",
        "typo3/cms-sys-note": "*",
        "typo3/cms-t3editor": "*",
        "typo3/cms-tstemplate": "*",
        "typo3/cms-viewpage": "*",
        "typo3/cms-webhooks": "*"
	},
  "scripts": {
    "typo3-cms-scripts": [
      "vendor/bin/typo3 install:fixfolderstructure"
    ],
    "post-autoload-dump": [
      "@typo3-cms-scripts"
    ]
  }
}
